/* 三角翻转 */
.rotate0{
  transform: rotate(0deg);
}
.rotate180{
  transform: rotate(180deg);
}
@keyframes triangle1{
    from {
        transform: rotate(0deg);
    }
    to {
        transform: rotate(180deg);
    }
}
@keyframes triangle3{
    from {
        transform: rotate(180deg);
    }
    to {
        transform: rotate(0deg);
    }
}