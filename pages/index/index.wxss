  width: 100%;
  height: 490rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.swiperBox{
  width: 690rpx;
  height: 324rpx;
}
.test5Img{
  width: 690rpx;
  height: 286rpx;
}
.swiperBox .wx-swiper-dots.wx-swiper-dots-horizontal{
    /* margin-bottom: 2rpx; */
    position: absolute;
    bottom:0rpx;
}
.test6Img{
  width: 100%;
