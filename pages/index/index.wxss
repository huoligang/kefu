.continer{
  display: flex;
  align-items: center;
  flex-direction: column;
}
.box1{
  width: 100%;
  height: 490rpx;
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.test1Img{
  width: 100%;
  height: 490rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.box1Section{
  width: 690rpx;
  height: 134rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 260rpx;
}
.box1List{
  width: 134rpx;
  height: 134rpx;
  border-radius: 50%;
  border: 1px solid #6f6f6f;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  font-size: 22rpx;
  color: #ffffff;
}
.text1{
  color: #ca4436;
}
.text1,.text2{
  line-height: 30rpx;
}
.box2{
  width: 100%;
  height: 510rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.titleBox{
  font-size: 34rpx;
  line-height: 34rpx;
  color: #333333;
}
.red{
  color: #c04032;
}
.test2Img{
  width: 673rpx;
  height: 336rpx;
}
.test3Img{
  width: 691rpx;
  height: 339rpx;
}
.test4Img{
